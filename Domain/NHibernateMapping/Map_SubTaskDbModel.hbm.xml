<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TODO_List"
                   namespace="TODO_List.Domain.Model">

	<class name="NH_SubTaskDbModel" table="NH_SubTasks">

		<id name="SubTaskId" column="SubTaskId">
			<generator class="assigned" />
		</id>

		<property name="SubTaskName" column="SubTaskName" />

		<many-to-one name="Task"
					 class="TODO_List.Domain.Model.NH_TaskDbModel"
					 column="TaskId"
					 not-null="true" />
	</class>

</hibernate-mapping>
